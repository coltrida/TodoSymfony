{% extends "base.html.twig" %}

{% block body %}
    {#{{ form(form) }}#}

    {{ form_start(form) }}
    {{ form_errors(form.priority) }}
    <div class="row">
        <div class="col-6">
            {{ form_label(form.priority, 'Priority') }}
            {{ form_row(form.priority) }}
        </div>
        <div class="col-6">
            {{ form_row(form.name) }}
        </div>
        <div class="col-6">
            {{ form_row(form.status) }}
        </div>
    </div>
    {{ form_end(form) }}

{% endblock %}






